{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "test:cov": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "create:migration": {
      "cache": false
    },
    "generate:migration": {
      "cache": false
    },
    "run:migration": {
      "cache": false
    },
    "revert:migration": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
